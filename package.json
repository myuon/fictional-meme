{
  "name": "fictional-meme",
  "version": "1.0.0",
  "description": "",
  "main": "app/main.js",
  "scripts": {
    "predev": "rimraf dist",
    "dev": "run-p dev:*",
    "dev:webpack": "yarn build:dev:webpack --watch",
    "dev:electron": "wait-on ./dist/index.html && cross-env NODE_ENV=development electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "build:dev:webpack": "cross-env NODE_ENV=development webpack",
    "build:prod:webpack": "corss-env NODE_ENV=production webpack",
    "lint": "run-p lint:*",
    "lint:ts": "eslint src/**/*.ts",
    "format": "run-p format:*",
    "format:eslint": "eslint --fix src/**/*.{ts,js}",
    "format:prettier": "prettier --write '{src,app}/**/*.{ts,js}'",
    "typecheck": "tsc",
    "codegen": "graphql-codegen --config codegen.yml"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/myuon/fictional-meme.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/myuon/fictional-meme/issues"
  },
  "homepage": "https://github.com/myuon/fictional-meme#readme",
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@babel/preset-env": "^7.17.10",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@electron-forge/cli": "^6.0.0-beta.63",
    "@electron-forge/maker-deb": "^6.0.0-beta.63",
    "@electron-forge/maker-rpm": "^6.0.0-beta.63",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.63",
    "@electron-forge/maker-zip": "^6.0.0-beta.63",
    "@emotion/babel-plugin": "^11.9.2",
    "@graphql-codegen/cli": "2.6.2",
    "@graphql-codegen/introspection": "2.1.1",
    "@graphql-codegen/typescript": "2.4.10",
    "@graphql-codegen/typescript-graphql-request": "^4.4.7",
    "@graphql-codegen/typescript-operations": "2.3.7",
    "@graphql-codegen/typescript-react-apollo": "3.2.13",
    "@types/react": "^18.0.8",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "babel-loader": "^8.2.5",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "electron": "^18.2.0",
    "electron-reload": "^2.0.0-alpha.1",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-css-reorder": "^0.5.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.5.0",
    "eslint-plugin-typescript": "^0.14.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^7.0.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "style-loader": "^3.3.1",
    "typescript": "^4.6.4",
    "wait-on": "^6.0.1",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2"
  },
  "dependencies": {
    "@apollo/client": "^3.6.2",
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@mui/icons-material": "^5.6.2",
    "@mui/material": "^5.6.4",
    "@octokit/graphql": "^4.8.0",
    "@types/react-dom": "^18.0.3",
    "@uiball/loaders": "^1.2.6",
    "core-js": "3",
    "dayjs": "^1.11.1",
    "electron-squirrel-startup": "^1.0.0",
    "graphql": "^16.4.0",
    "graphql-request": "^4.2.0",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "ress": "^5.0.2",
    "swr": "^1.3.0"
  },
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "fictional_meme"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  }
}
